<%= simple_form_for [@domain, @course, @class_note] do |f| %>
  <%= f.input :title %>
  <%= f.input :author %>
  <%= f.association :course %>
  <%= f.input :content, required: true %>
  <%= f.input :image %>
  <%= f.input :image_cache, as: :hidden %>
  <%= f.submit "Submit", class: "btn btn-primary" %>
<% end %>
